{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.3",
        "symfony/runtime": "^7.3",
        "symfony/dotenv": "^7.3",
        "symfony/yaml": "^7.3",
        "symfony/console": "^7.3",
        "symfony/messenger": "^7.3",
        "symfony/routing": "^7.3",
        "symfony/config": "^7.3",
        "doctrine/orm": "^3.0",
        "symfony/validator": "*"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "*",
        "phpunit/phpunit": "*",
        "ramsey/uuid": "^4.9",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/maker-bundle": "^1.64",
        "symfony/phpunit-bridge": "^7.3"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "php bin/console cache:clear",
            "assets:install %PUBLIC_DIR%": "php bin/console assets:install --symlink --relative public"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": "vendor/bin/phpunit",
        "cov": "@test-coverage",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-html .coverage"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
